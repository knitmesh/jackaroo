@startuml
package com.iluwatar.doublechecked.locking {
  class App {
    - LOGGER : Logger {static}
    + App()
    + main(args : String[]) {static}
  }
  class Inventory {
    - LOGGER : Logger {static}
    - inventorySize : int
    - items : List<Item>
    - lock : Lock
    + Inventory(inventorySize : int)
    + addItem(item : Item) : boolean
    + getItems() : List<Item>
  }
  class Item {
    + Item()
  }
}
Inventory -->  "-items" Item
@enduml